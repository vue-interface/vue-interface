    "change-case": "^4.1.2",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "pascalcase": "^2.0.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.2.37"
