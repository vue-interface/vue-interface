{
  "name": "@vue-interface/activity-indicator",
  "version": "0.9.9",
  "description": "An Vue activity indicator component.",
  "main": "index.js",
  "scripts": {
    "watch": "npm run serve",
    "serve": "vue-cli-service build --target lib --name=ActivityIndicator ./main.vue --watch",
    "build": "vue-cli-service build --target lib --name=ActivityIndicator ./main.vue",
    "lint": "vue-cli-service lint",
    "fix": "vue-cli-service lint --fix",
    "pre-release": "npm run build; git add . -A; git commit -m 'pre-release commit'",
    "release-patch": "npm run pre-release && npm version patch -m \"%s\" && npm run release;",
    "release-minor": "npm run pre-release && npm version minor -m \"%s\" && npm run release;",
    "release-major": "npm run pre-release && npm version major -m \"%s\" && npm run release;",
    "release": "git add . -A; git commit; git push --tags origin; npm publish;",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "./node_modules/.bin/vuepress dev docs",
    "docs:build": "./node_modules/.bin/vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-interface/activity-indicator.git"
  },
  "keywords": [
    "Form",
    "Control",
    "Vue",
    "Mixin",
    "Tailwind",
    "Bootstrap"
  ],
  "author": "Justin Kimbrell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vue-interface/activity-indicator/issues"
  },
  "homepage": "https://github.com/vue-interface/activity-indicator/docs#readme",
  "dependencies": {
    "@vue-interface/utils": "^0.9.7",
    "core-js": "^3.6.4",
    "lodash-es": "^4.17.15",
    "lodash.kebabcase": "^4.1.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vuepress/plugin-register-components": "^1.5.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "markdown-it-meta": "0.0.1",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.2",
    "vuepress-plugin-template-constants": "^0.2.1"
  }
}
